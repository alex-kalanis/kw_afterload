{
    "name": "alex-kalanis/kw_afterload",
    "description": "Autoloading configurations by separate files",
    "keywords": [
        "config",
        "configuration",
        "autoload",
        "afterload",
        "bootstrap",
        "autoloading"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_afterload\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
